@page
@model BudgetRequests.Pages.CreateSuperAdmin.IndexModel

@{
    ViewData["Title"] = "Create Super Admin";
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
<div>
    <div class="row">
        <div class="col-md-4">
            <form method="post">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="FirstName" class="control-label"></label>
                    <input asp-for="FirstName" class="form-control mt-1"/>
                    <span asp-validation-for="FirstName" class="text-danger mt-1"></span>
                </div>
                <div class="form-group mt-2">
                    <label asp-for="MiddleName" class="control-label"></label>
                    <input asp-for="MiddleName" class="form-control mt-1"/>
                    <span asp-validation-for="MiddleName" class="text-danger mt-1"></span>
                </div>
                <div class="form-group mt-2">
                    <label asp-for="LastName" class="control-label"></label>
                    <input asp-for="LastName" class="form-control mt-1"/>
                    <span asp-validation-for="LastName" class="text-danger mt-1"></span>
                </div>
                <div class="form-group mt-2">
                    <label asp-for="Username" class="control-label"></label>
                    <input asp-for="Username" class="form-control mt-1"/>
                    <span asp-validation-for="Username" class="text-danger mt-1"></span>
                </div>
                <div class="form-group mt-2">
                    <label asp-for="Password" class="control-label"></label>
                    <input asp-for="Password" class="form-control mt-1"/>
                    <span asp-validation-for="Password" class="text-danger mt-1"></span>
                </div>
                <div class="form-group mt-2">
                    <input type="submit" value="Save" class="btn btn-primary"/>
                </div>
            </form>
        </div>
    </div>
</div>
</body>
</html>